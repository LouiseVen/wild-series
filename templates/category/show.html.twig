{% extends 'base.html.twig' %}

{% block title %}Hello CategoryController!{% endblock %}

{% block body %}

    <h1>{{ categories.name }}</h1>
    {% for program in programs %}
        <a href="{{ path('program_show', {id: program.id}) }}">
        <div class="card-body">
            <h3>{{ program.title }}</h3>
            <p>{{ program.synopsis}}</p>
        </div>
        </a>
    {% endfor %}
{% endblock %}
